{
    "yaml.validate": false,
    "python.formatting.provider": "black",
    "coc.preferences.formatOnSaveFiletypes": ["python", "javascript", "terraform"],
    "languageserver": {
        "terraform": {
            "command": "terraform-ls",
            "args": ["serve"],
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {},
            "settings": {}
        }
    },
    "eslint.autoFixOnSave": true
}
